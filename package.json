{
  "name": "private-monorepo",
  "version": "0.0.1",
  "private": true,
  "packageManager": "pnpm@10.17.0",
  "scripts": {
    "build": "pnpm -r build",
    "dev": "pnpm -r --parallel --stream dev",
    "start": "pnpm --filter basic-example start",
    "publish": "pnpm --filter fastify-file-router publish",
    "clean": "rimraf dist packages/**/dist demos/**/dist .nx node_modules packages/**/node_modules demos/**/node_modules",
    "check": "biome check --write",
    "tsc": "pnpm -r tsc",
    "cloc": "npx cloc * --exclude-dir=node_modules,dist,.nx,.next,build"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.4",
    "@types/node": "^24",
    "rimraf": "^6.0.1",
    "typescript": "^5"
  },
  "engines": {
    "node": ">=24.0.0"
  }
}
