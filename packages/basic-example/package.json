{
  "name": "basic-example",
  "type": "module",
  "src": "src/index.ts",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch || node --no-warnings --experimental-strip-types --experimental-transform-types --watch src/server.ts",
    "start": "node --no-warnings --experimental-strip-types --experimental-transform-types src/server.ts",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write .",
    "lint": "eslint . --ext .ts --fix"
  },
  "dependencies": {
    "fastify": "^5.0.0",
    "fastify-file-router": "*",
    "json-schema-to-ts": "^3.1.1"
  }
}
